                        </Trigger>
                        <Trigger Property="WindowState" Value="Normal">
                            <Setter Property="IsEnabled" TargetName="Normal" Value="False"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="NoTransparentWindowStyle" TargetType="{x:Type Window}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="WindowStyle" Value="None"/>
        <Setter Property="ResizeMode" Value="NoResize"/>
        <Setter Property="Background" Value="{DynamicResource PopupWindow_Background}"/>
        <Setter Property="Foreground" Value="{DynamicResource PopupWindow_Foreground}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Window}">
                    <Grid>
                        <Grid Name="windowContentGrid"  Margin="0,30,0,0">
                            <AdornerDecorator>
                                <ContentPresenter/>
                            </AdornerDecorator>
                        </Grid>
                        <!--标题条和窗口按钮-->
                        <Grid Name="titlebarGrid" TextBlock.Foreground="{DynamicResource PopupWindow_Title_Foreground}" Background="{DynamicResource PopupWindow_Title_Background}"
                              MouseLeftButtonDown="Grid_MouseLeftButtonDown" MouseMove="Grid_MouseMove" PreviewMouseLeftButtonDown="Grid_PreviewMouseLeftButtonDown" VerticalAlignment="Top" Height="30">
                            <Rectangle HorizontalAlignment="Stretch" Height="1" Width="Auto" VerticalAlignment="Bottom" Margin="0,0,0,-1" Opacity="0.1" Fill="Black"/>
                            <TextBlock HorizontalAlignment="Left" Height="Auto" TextWrapping="Wrap" Text="{TemplateBinding Title}" VerticalAlignment="Center" Width="Auto" Margin="10,0,0,0" 
                                       Foreground="{TemplateBinding Foreground}" FontSize="14" Opacity="0.8"/>
                            <WrapPanel HorizontalAlignment="Right" Height="30" VerticalAlignment="Top" Width="Auto" Background="Transparent">
                                <!--<Button x:Name="Minimized" Height="30" Width="40" Click="Minimized_Click" Style="{DynamicResource Button_Window_Minimized}" Foreground="{TemplateBinding Foreground}"/>-->
                                <Button x:Name="Normal" Height="30" Width="40" Click="Normal_Click" Style="{DynamicResource Button_Window_Normal}" Foreground="{TemplateBinding Foreground}"/>
                                <Button x:Name="Maximized" Height="30" Width="40" Click="Maximized_Click" Style="{DynamicResource Button_Window_Maximized}" Foreground="{TemplateBinding Foreground}"/>
                                <Button x:Name="Close" Height="30" Width="40" Click="Close_Click" Style="{DynamicResource Button_Window_Close}" Foreground="{TemplateBinding Foreground}"/>
                            </WrapPanel>
                        </Grid>
                        <!--尺寸调整区域-->
                        <Grid x:Name ="PART_ResizerContainers" Visibility ="Hidden">
                            <Grid.Resources >
                                <s:Double x:Key ="StraightResizerSize">8</s:Double >
                                <s:Double x:Key ="SlantResizerSize">16</s:Double >
                                <Style TargetType ="{ x:Type Thumb }">
                                    <Setter Property ="Template">
                                        <Setter.Value >
                                            <ControlTemplate >
                                                <Rectangle Fill ="Transparent"/>
                                            </ControlTemplate >
                                        </Setter.Value >
                                    </Setter >
                                </Style>
                            </Grid.Resources >
                            <Thumb Width ="{ StaticResource StraightResizerSize }" VerticalAlignment ="Stretch" HorizontalAlignment="Left" 
                                    Cursor ="SizeWE"  x:Name ="PART_LeftResizer" DragDelta="ResizeLeft"/>
                            <Thumb Height ="{ StaticResource StraightResizerSize }" VerticalAlignment ="Top" HorizontalAlignment="Stretch"
                                    Cursor ="SizeNS"  x:Name ="PART_TopResizer" DragDelta="ResizeTop"/>
                            <Thumb Width ="{ StaticResource StraightResizerSize }" VerticalAlignment ="Stretch" HorizontalAlignment="Right"
                                    Cursor ="SizeWE" x:Name ="PART_RightResizer" DragDelta="ResizeRight"/>
                            <Thumb Height ="{ StaticResource StraightResizerSize }" VerticalAlignment ="Bottom" HorizontalAlignment="Stretch"
                                    Cursor ="SizeNS"  x:Name ="PART_BottomResizer" DragDelta="ResizeBottom"/>

                            <ResizeGrip Width ="{ StaticResource SlantResizerSize }" Height ="{ StaticResource SlantResizerSize }" HorizontalAlignment ="Right" VerticalAlignment ="Bottom" />
                            <Thumb Width ="{ StaticResource SlantResizerSize }" Height ="{ StaticResource SlantResizerSize }" 
                            HorizontalAlignment ="Right" VerticalAlignment ="Bottom" 
                            Cursor ="SizeNWSE"  x:Name ="PART_BottomRightResizer" DragDelta="ResizeBottomRight"/>
                            <Thumb Width ="{ StaticResource SlantResizerSize }" Height ="{ StaticResource SlantResizerSize }" 
                            HorizontalAlignment ="Right" VerticalAlignment ="Top" 
                            Cursor ="SizeNESW"  x:Name ="PART_TopRightResizer" DragDelta="ResizeTopRight"/>
                            <Thumb Width ="{ StaticResource SlantResizerSize }" Height ="{ StaticResource SlantResizerSize }"
                            HorizontalAlignment ="Left" VerticalAlignment ="Top"
                            Cursor ="SizeNWSE"  x:Name ="PART_TopLeftResizer" DragDelta="ResizeTopLeft"/>
                            <Thumb Width ="{ StaticResource SlantResizerSize }" Height ="{ StaticResource SlantResizerSize }" 
                            HorizontalAlignment ="Left" VerticalAlignment ="Bottom"
                            Cursor ="SizeNESW"  x:Name ="PART_BottomLeftResizer" DragDelta="ResizeBottomLeft"/>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="WindowState" Value="Maximized">
                            <Setter Property="IsEnabled" TargetName="Maximized" Value="False"/>
                        </Trigger>
                        <Trigger Property="WindowState" Value="Normal">
                            <Setter Property="IsEnabled" TargetName="Normal" Value="False"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>
