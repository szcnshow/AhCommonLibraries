<ResourceDictionary x:Class="Ai.Hong.CommonResources.Styles.WindowStyle"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:y="clr-namespace:System.Windows;assembly=PresentationFramework"
                    xmlns:s="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:Ai.Hong.CommonResources.Styles"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <local:BrushToColorConverter x:Key="colorConvert"/>
    
    <Style x:Key="Window" TargetType="{x:Type Window}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <!--<Setter Property="MinWidth" Value="300"/>
        <Setter Property="MinHeight" Value="300"/>-->
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="AllowsTransparency" Value="True"/>
        <Setter Property="WindowStyle" Value="None"/>
        <Setter Property="FontFamily" Value="Consolas,Microsoft YaHei UI"/>
        <Setter Property="TextOptions.TextFormattingMode" Value="Display"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Window}">                    
                    <Border x:Name="WindowGrid" CornerRadius="10" Background="{TemplateBinding Background}" Margin="10" ClipToBounds="False">
                        <Border.Effect>
                            <DropShadowEffect BlurRadius="15" Direction="-90" Opacity="0.6" ShadowDepth="0"/>
                        </Border.Effect>
                        <Grid>
                        <Grid ClipToBounds="True">
                            <Grid Margin="0,30,0,0">
                                <AdornerDecorator>
                                    <ContentPresenter/>
                                </AdornerDecorator>
                            </Grid>
                            <Grid Background="#0C000000"  MouseLeftButtonDown="Grid_MouseLeftButtonDown" MouseMove="Grid_MouseMove" PreviewMouseLeftButtonDown="Grid_PreviewMouseLeftButtonDown" VerticalAlignment="Top" Height="30">
                                <Rectangle HorizontalAlignment="Stretch" Height="1" Width="Auto" VerticalAlignment="Bottom" Margin="0,0,0,-1" Opacity="0.1" Fill="Black"/>
                                <TextBlock HorizontalAlignment="Left" Height="Auto" TextWrapping="Wrap" Text="{TemplateBinding Title}" VerticalAlignment="Center" Width="Auto" Margin="10,0,0,0" Foreground="{TemplateBinding Foreground}" FontSize="14" Opacity="0.8"/>
                                <WrapPanel HorizontalAlignment="Right" Height="30" VerticalAlignment="Top" Width="Auto" >
                                        <Button x:Name="Minimized" Height="30" Width="40" Click="Minimized_Click" Style="{DynamicResource Button_Window_Minimized}" Foreground="{TemplateBinding Foreground}"/>
                                        <Button x:Name="Normal" Height="30" Width="40" Click="Normal_Click" Style="{DynamicResource Button_Window_Normal}" Foreground="{TemplateBinding Foreground}"/>
                                    <Button x:Name="Maximized" Height="30" Width="40" Click="Maximized_Click" Style="{DynamicResource Button_Window_Maximized}" Foreground="{TemplateBinding Foreground}"/>
                                        <Button x:Name="Close" Height="30" Width="40" Click="Close_Click" Style="{DynamicResource Button_Window_Close}" Foreground="{TemplateBinding Foreground}"/>
                                </WrapPanel>
                            </Grid>
                        </Grid>
                        <Border BorderBrush="{TemplateBinding Foreground}" Opacity="0.06" BorderThickness="1"/>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        
                        <Trigger Property="IsActive" Value="False">
                            <Setter Property="Effect" TargetName="WindowGrid">
                                <Setter.Value>
                                    <DropShadowEffect BlurRadius="8" Direction="-90" Opacity="0.2" ShadowDepth="0"/>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        
                        <Trigger Property="WindowState" Value="Maximized">
                            <Setter Property="Margin" TargetName="WindowGrid" Value="0"/>
                            <Setter Property="IsEnabled" TargetName="Maximized" Value="False"/>
                        </Trigger>
                        <Trigger Property="WindowState" Value="Normal">
                            <Setter Property="IsEnabled" TargetName="Normal" Value="False"/>
                        </Trigger>
                        <Trigger Property="ResizeMode" Value="NoResize">
                            <Setter Property="IsEnabled" TargetName="Normal" Value="False"/>
                            <Setter Property="IsEnabled" TargetName="Maximized" Value="False"/>
                            <Setter Property="IsEnabled" TargetName="Minimized" Value="False"/>
                        </Trigger>
                        <Trigger Property="ResizeMode" Value="CanMinimize">
                            <Setter Property="IsEnabled" TargetName="Normal" Value="False"/>
                            <Setter Property="IsEnabled" TargetName="Maximized" Value="False"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
