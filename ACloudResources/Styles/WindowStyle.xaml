    <Style x:Key="Button_Window_Maximized" TargetType="{x:Type Button}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid x:Name="grid1" Background="#00000000">
                        <y:VisualStateManager.VisualStateGroups>
                            <y:VisualStateGroup x:Name="CommonStates">
                                <y:VisualStateGroup.Transitions>
                                    <y:VisualTransition GeneratedDuration="0:0:0.15"/>
                                </y:VisualStateGroup.Transitions>
                                <y:VisualState x:Name="Normal"/>
                                <y:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="grid1">
                                            <EasingColorKeyFrame KeyTime="0" Value="{Binding Source={StaticResource  WindowTitleButton_SelectedColor}, Converter={StaticResource colorConvert}}"/>
                                        </ColorAnimationUsingKeyFrames>
                                    </Storyboard>
                                </y:VisualState>
                                <y:VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="grid1">
                                            <EasingColorKeyFrame KeyTime="0" Value="{Binding Source={StaticResource  WindowTitleButton_SelectedColor}, Converter={StaticResource colorConvert}}"/>
                                        </ColorAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Canvas.Top)" Storyboard.TargetName="grid">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="2"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </y:VisualState>
                                <y:VisualState x:Name="Disabled"/>
                            </y:VisualStateGroup>
                        </y:VisualStateManager.VisualStateGroups>
                        <Canvas HorizontalAlignment="Center" VerticalAlignment="Center">
                            <Grid x:Name="grid" Height="Auto" Width="Auto" VerticalAlignment="Stretch" Canvas.Left="0" Canvas.Top="0" Margin="-6,-6,0,0">
                                <Rectangle x:Name="rectangle" Fill="{x:Null}" HorizontalAlignment="Center" Height="12" VerticalAlignment="Center" Width="12" 
                                           Stroke="{DynamicResource WindowTitleButton_Foreground}" Opacity="0.6"/>
                            </Grid>
                        </Canvas>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Visibility" Value="Hidden"/>
                            <Setter Property="MaxWidth" Value="0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <Style x:Key="Button_Window_Close" TargetType="{x:Type Button}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid x:Name="grid" Background="#00000000">
                        <y:VisualStateManager.VisualStateGroups>
                            <y:VisualStateGroup x:Name="CommonStates">
                                <y:VisualStateGroup.Transitions>
                                    <y:VisualTransition GeneratedDuration="0:0:0.15"/>
                                </y:VisualStateGroup.Transitions>
                                <y:VisualState x:Name="Normal"/>
                                <y:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="grid">
                                            <EasingColorKeyFrame KeyTime="0" Value="#50FF0000"/>
                                        </ColorAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="rectangle">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="rectangle1">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </y:VisualState>
                                <y:VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="grid">
                                            <EasingColorKeyFrame KeyTime="0" Value="Red"/>
                                        </ColorAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Canvas.Top)" Storyboard.TargetName="grid1">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="2"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </y:VisualState>
                                <y:VisualState x:Name="Disabled"/>
                            </y:VisualStateGroup>
                        </y:VisualStateManager.VisualStateGroups>
                        <Canvas VerticalAlignment="Center" HorizontalAlignment="Center">
                            <Grid x:Name="grid1" RenderTransformOrigin="0.5,0.5" Height="Auto" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="-8,-8,0,0" Canvas.Left="0" Canvas.Top="0">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform Angle="45"/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Grid.RenderTransform>
                                <Rectangle x:Name="rectangle" Fill="{DynamicResource WindowTitleButton_Foreground}" HorizontalAlignment="Center" Height="1" VerticalAlignment="Center" Width="16" Opacity="0.6"/>
                                <Rectangle x:Name="rectangle1" Fill="{DynamicResource WindowTitleButton_Foreground}" HorizontalAlignment="Center" Height="16" VerticalAlignment="Center" Width="1" Opacity="0.6"/>
                            </Grid>
                        </Canvas>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
